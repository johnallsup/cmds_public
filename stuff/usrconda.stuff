# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/usr/anaconda/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/usr/anaconda/etc/profile.d/conda.sh" ]; then
        . "/usr/anaconda/etc/profile.d/conda.sh"
    else
        export PATH="/usr/anaconda/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

